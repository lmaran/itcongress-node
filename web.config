<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>

    <modules runAllManagedModulesForAllRequests="false" />

    <iisnode watchedFiles="web.config;*.js"/>

    <handlers>
      <add name="iisnode" path="server.js" verb="*" modules="iisnode" />
    </handlers>

  </system.webServer>
</configuration>
